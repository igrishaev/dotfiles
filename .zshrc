# Python
#

export PIP_DISABLE_PIP_VERSION_CHECK=1

#
# GPG
#

GNUPGHOME=~/.gnupg
GPG_TTY=$(tty)
export GPG_TTY


function whoseport(){
    lsof -i 4tcp:$1 -sTCP:LISTEN
}

function gen_pass(){
    pwgen -N 1 -cn 20
}

function del_orig(){
    find . -name '*.orig' -delete
}

# https://irreal.org/blog/?p=4763
function emacs_wakeup(){
    pkill -SIGUSR2 Emacs
}

function emacs_term(){
    pkill -SIGTERM Emacs
}


function git_clean(){
    git clean -df
}


